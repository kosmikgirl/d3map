{
  "name": "lit-scaffold",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "lit-localize build && rollup -c",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint 'src/**/*.{js,ts}'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "plop": "plop",
    "locale:extract": "lit-localize extract",
    "pwa:build": "lit-localize build && rollup -c --isPwa && npm run pwa:sw-generate",
    "pwa:sw-generate": "workbox generateSW workbox-config.js"
  },
  "dependencies": {
    "@lit/localize": "^0.11.1",
    "@reduxjs/toolkit": "^1.6.2",
    "lit": "^2.0.0",
    "navigo": "^8.11.1",
    "pwa-helpers": "^0.9.1",
    "seng-device-state-tracker": "^1.1.4"
  },
  "devDependencies": {
    "@lit/localize-tools": "^0.5.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/navigo": "^8.1.2",
    "@types/node": "^14.17.34",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.10.1",
    "eslint": "^8.2.0",
    "eslint-plugin-lit-a11y": "^1.1.0-next.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gts": "^3.1.0",
    "imagemin-webp": "^6.0.0",
    "lit-analyzer": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "plop": "^2.1.0",
    "prettier": "^2.4.1",
    "rollup": "^2.60.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-imagemin": "^0.4.1",
    "rollup-plugin-summary": "^1.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-lit-plugin": "^1.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "vite": "^2.6.4",
    "workbox-cli": "^6.4.1"
  }
}
